function urlQueryString(queryUrl) {
    let queryLink = queryUrl.substring(1);
    if (queryLink.indexOf('#') > -1) {
        queryLink = queryLink.split('#')[1];
    }
    let pairs = queryLink.split('&');
    let result = {};
    pairs.forEach(function (pair) {
        pair = pair.split('=');
        result[pair[0]] = decodeURIComponent(pair[1] || '');
    });
    return result;
}

