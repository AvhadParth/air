async function encryptMemebershipId(){
    let aiEnv = decode(window.sessionStorage.getItem("env")),
    ltyOcpKey = decode(window.sessionStorage.getItem("ltyOcp")),
    accessToken = decode(window.sessionStorage.getItem('accessToken')),
    headerObj = {
        'Accept': 'application/json',
        'Access-Control-Allow-Origin': '*',
        'Content-Type': 'application/json',
        'Authorization': 'Bearer ' + accessToken,
        'Ocp-Apim-Subscription-Key': ltyOcpKey,
        'X-Request-Source': 'website'
    };
    let encryptUrl =  "dev" === aiEnv ? AIEnvUrl.dev.BASE_URL + AIEnvUrl.dev.ENCRYPT_MEMBERSHIP : "qa" === aiEnv ? AIEnvUrl.qa.BASE_URL + AIEnvUrl.dev.ENCRYPT_MEMBERSHIP : "stage" === aiEnv ? AIEnvUrl.stage.BASE_URL + AIEnvUrl.dev.ENCRYPT_MEMBERSHIP : "prod" === aiEnv && (AIEnvUrl.prod.BASE_URL + AIEnvUrl.dev.ENCRYPT_MEMBERSHIP);
    await fetch(encryptUrl, {
        method: 'GET',
        headers: headerObj,
    }).then(response => response.json())
    .then(data => {
        encryptMemberId = data.responsePayload.membershipId;
        redirectToApplePage();
    });
}

$('#partner-offer-apple .book-btn').click(function(e){
    e.preventDefault();
    partnerBuyNow();
})

function partnerBuyNow(){
    if(!window.sessionStorage.getItem('accessToken')){
        $("#partnerSigninModal").modal("show");
    }else{
        if(encryptMemberId !== ''){
            redirectToApplePage();
        }else{
            encryptMemebershipId();
        }
    }
}

function redirectToApplePage(){
    window.open(appleBuyNowUrl + '?subId1=' + encryptMemberId, '_blank');
}

function partnerSignInContinue(){
    window.sessionStorage.setItem('prtnrByNw', encode('true'));
    signIn();
}

function changeRowInConsolidatedTab(){
    $('#partner-offer-apple #consolidated-offers-afterhwTerm .consolidatedTab .row').removeClass('row-cols-md-3 row-cols-lg-4');
    $('#partner-offer-apple #consolidated-offers-afterhwTerm .consolidatedTab .row').addClass('row-cols-md-2 row-cols-lg-2');
}

function animateGift() {
    var elements = document.querySelectorAll('.top-box, .top-left, .top-right, .top-box-line');

    elements.forEach(function (element) {
        element.classList.add('animate');
    });

    // Create the bottom box animate element
    var pathElementBottomBox = document.querySelectorAll("[aria-label='ai-gift-card-icon-bottom-box']");
    // Append the animate element to the path element
    pathElementBottomBox.forEach(item => {
        item.innerHTML= '<animate attributeName="d" dur="2.25s" values="M17.4534 11.9165V18.8332H6.38672V11.9165 H7 H17; M17.4534 14.9165V18.8332H6.38672V14.9165 H7 H17; M17.4534 12.9165V18.8332H6.38672V12.9165 H7 H17; M17.4534 11.9165V18.8332H6.38672V11.9165 H7 H17; M17.4534 11.9165V18.8332H6.38672V11.9165 H7 H17" repeatCount="3" />';
    });

    // Create the middle line animate element
    var pathElementBottomLine = document.querySelectorAll("[aria-label='ai-gift-card-icon-middle-line']");
    // Set the attributes of the animate element
    pathElementBottomLine.forEach(item => {
        item.innerHTML= '<animate attributeName="d" dur="2.25s" values="M11.918 18.833V11.916;M11.918 18.833V14.916;M11.918 18.833V12.916;M11.918 18.833V11.916;M11.918 18.833V11.916" repeatCount="3" />';
    });


}

